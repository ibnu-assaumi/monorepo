{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "get/master/api/v2/soprefix",
    "title": "JSON Schema for get all list SO prefix",
    "type": "object",
    "properties": {
        "page": {
            "type": "number",
            "default": 1,
            "minimum": 0
        },
        "limit": {
            "type": "number",
            "default": 10,
            "minimum": 1
        },
        "orderBy": {
            "type": "string",
            "enum": ["id", "code", "description", "createdAt", "modifiedAt"]
        },
        "sort": {
            "type": "string",
            "enum": ["asc", "desc"]
        },
        "search": {
            "type": "string"
        }
    },
    "additionalProperties": true
}